{
  "manifest_version": 3,
  "name": "AnimeStars Alpha",
  "version": "1.2.2",
  "author": "MrKoMi",
  "description": "Press INSERT to open the menu",
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://animestars.org/*"
  ],
  "background": {
    "service_worker": "background/worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://animestars.org/*"
      ],
      "js": [
        "content/libs/config.js",
        "menu/style/menu.js",
        "menu/components.js",
        "menu/tab.js",
        "menu/tabs/misc.js",
        "menu/tabs/settings.js",
        "menu/menu.js",
        "menu/content.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/*"
      ],
      "css": [
        "css/button.css",
        "css/switcher.css"
      ],
      "js": [
        "content/libs/getUserHash.js",
        "content/libs/localStorage.js",
        "content/libs/util.js",
        "content/libs/fetchApi.js",
        "content/getCard.js",
        "content/theme/globalColors.js",
        "content/libs/CardClasses.js",
        "content/libs/container.js",
        "content/libs/cardFinder.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/users/need/"
      ],
      "js": [
        "content/need.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/users/trade/"
      ],
      "js": [
        "content/trade.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/users/"
      ],
      "js": [
        "content/users.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards_remelt/*"
      ],
      "js": [
        "content/remelt.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/trade?mycard=*"
      ],
      "js": [
        "content/tradeUser.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/clubs/*/boost/*"
      ],
      "js": [
        "content/clubs.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/user/*/cards/trade/*"
      ],
      "js": [
        "content/myPropose.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/trades/"
      ],
      "js": [
        "content/confirmTrade.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/?rank=s"
      ],
      "js": [
        "content/add-colodes.js"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}