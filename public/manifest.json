{
  "manifest_version": 3,
  "name": "AnimeStars Alpha",
  "version": "1.3.10",
  "author": "MrKoMi",
  "description": "Press INSERT to open the menu",
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "https://animestars.org/*",
    "https://asstars.tv/*",
    "https://as1.astars.club/*",
    "https://asstars1.astars.club/*"
  ],
  "background": {
    "service_worker": "background/worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://animestars.org/*",
        "https://asstars.tv/*",
        "https://as1.astars.club/*",
        "https://asstars1.astars.club/*"
      ],
      "js": [
        "content/services/config.js",
        "menu/style/menu.js",
        "menu/components.js",
        "menu/tab.js",
        "menu/tabs/misc.js",
        "menu/tabs/fun.js",
        "menu/tabs/global.js",
        "menu/tabs/visuals.js",
        "menu/menu.js",
        "menu/content.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/*",
        "https://asstars.tv/*",
        "https://as1.astars.club/*",
        "https://asstars1.astars.club/*"
      ],
      "css": [
        "css/button.css",
        "css/switcher.css",
        "css/cards.css"
      ],
      "js": [
        "content/external/jQuery.js",
        "content/external/jQueryUI.js",
        "content/external/AnimeStarsjQuery.js",

        "content/theme/globalColors.js",

        "content/services/getUserHash.js",
        "content/services/localStorage.js",
        "content/services/SaveFetch.js",

        "content/services/fetch/FetchService.js",
        "content/services/fetch/ProtectedFetchService.js",

        "content/models/card/Card.js",
        "content/models/card/CardsArray.js",
        "content/models/card/CardsHash.js",
        "content/models/user/User.js",
        "content/models/user/UsersArray.js",

        "content/services/container.js",

        "content/components/ui/Element.js",
        "content/components/ui/Button.js",
        "content/components/ui/ShowBar.js",
        "content/components/ui/Switcher.js",
        "content/components/ui/Input.js",
        "content/components/ui/Li.js",
        "content/components/ui/Box.js",
        
        "content/services/urls/UrlService.js",
        "content/services/urls/AnimeUrlService.js",
        "content/services/urls/CardUrlService.js",
        "content/services/urls/TradeUrlService.js",
        "content/services/urls/UserUrlService.js",
        "content/services/urls/InventoryUrlService.js",
        "content/services/urls/UrlConstructor.js",

        "content/services/cache/CacheService.js",
        "content/services/cards/GetCards.js",
        "content/services/cards/CardsHelper.js",
        "content/services/cards/CardsFinder.js",

        "content/services/trades/TradeHelper.js",
        "content/services/trades/TradeMonitor.js",
        "content/services/trades/TradeHistory.js",

        "content/DataConfig.js",
        "content/ReceiveCard.js",

        "content/AutoLogin.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/users/need/",
        "https://asstars.tv/cards/*/users/need/",
        "https://as1.astars.club/cards/*/users/need/",
        "https://asstars1.astars.club/cards/*/users/need/"
      ],
      "js": [
        "content/Need.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/users/trade/",
        "https://asstars.tv/cards/*/users/trade/",
        "https://as1.astars.club/cards/*/users/trade/",
        "https://asstars1.astars.club/cards/*/users/trade/"
      ],
      "js": [
        "content/Trade.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/users/",
        "https://asstars.tv/cards/*/users/",
        "https://as1.astars.club/cards/*/users/",
        "https://asstars1.astars.club/cards/*/users/"
      ],
      "js": [
        "content/Users.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards_remelt/*",
        "https://asstars.tv/cards_remelt/*",
        "https://as1.astars.club/cards_remelt/*",
        "https://asstars1.astars.club/cards_remelt/*"
      ],
      "js": [
        "content/Remelt.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/*/trade?mycard=*",
        "https://asstars.tv/cards/*/trade?mycard=*",
        "https://as1.astars.club/cards/*/trade?mycard=*",
        "https://asstars1.astars.club/cards/*/trade?mycard=*"
      ],
      "js": [
        "content/TradeUser.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/clubs/*/boost/*",
        "https://asstars.tv/clubs/*/boost/*",
        "https://as1.astars.club/clubs/*/boost/*",
        "https://asstars1.astars.club/clubs/*/boost/*"
      ],
      "js": [
        "content/external/socketIo.js",
        "content/Clubs.js",
        "content/ClubsBoostServer.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/user/*/cards/trade/*",
        "https://asstars.tv/user/*/cards/trade/*",
        "https://as1.astars.club/user/*/cards/trade/*",
        "https://asstars1.astars.club/user/*/cards/trade/*"
      ],
      "js": [
        "content/Propose.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/trades/",
        "https://asstars.tv/trades/",
        "https://as1.astars.club/trades/",
        "https://asstars1.astars.club/trades/"
      ],
      "js": [
        "content/ConfirmTrade.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/cards/pack/",
        "https://asstars.tv/cards/pack/",
        "https://as1.astars.club/cards/pack/",
        "https://asstars1.astars.club/cards/pack/"
      ],
      "js": [
        "content/external/jQueryHTML.js",
        "content/Packs.js"
      ],
      "css": [
        "css/packs.css"
      ]
    },
    {
      "matches": [
        "https://animestars.org/user/*/",
        "https://asstars.tv/user/*/",
        "https://as1.astars.club/user/*/",
        "https://asstars1.astars.club/user/*/"
      ],
      "js": [
        "content/Profile.js"
      ]
    },
    {
      "matches": [
        "https://animestars.org/aniserials/videos/*/*",
        "https://asstars.tv/aniserials/videos/*/*",
        "https://as1.astars.club/aniserials/videos/*/*",  
        "https://asstars1.astars.club/aniserials/videos/*/*"
      ],
      "js": [
        "content/Deck.js"
      ]
    }, 
    {
      "matches": [
        "https://animestars.org/user/*/cards_progress/*",
        "https://asstars.tv/user/*/cards_progress/*",
        "https://as1.astars.club/user/*/cards_progress/*",
        "https://asstars1.astars.club/user/*/cards_progress/*"
      ],
      "js": [
        "content/external/exel.js",
        "content/services/downloadExel.js",
        "content/DecksProgressAnalyzer.js"
      ]
    }
  ]
}